{
    "name": "talbot-ninja/ssh-keys",
    "description": "Sync SSH-keys from GitHub, GitLab or Bitbucket with server",
    "keywords": ["ssh-keys", "github", "gitlab", "bitbucket"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Talbot",
            "email": "vincent.talbot@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "gentle/bitbucket-api": "^1.1",
        "knplabs/github-api": "^2.11",
        "laravel-zero/framework": "5.8.*",
        "m4tthumphrey/php-gitlab-api": "^9.12",
        "php-http/guzzle6-adapter": "^1.1",
        "symfony/process": "^4.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/ssh-keys"]
}
